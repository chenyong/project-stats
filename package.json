{
  "name": "@jimengio/project-stats",
  "version": "0.1.0-a4",
  "description": "Scripts for scanning project status",
  "main": "index.js",
  "scripts": {
    "watch": "shadow-cljs watch uppercase",
    "serve": "http-server dist -s",
    "build-cdn": "shadow-cljs clj-run build.main/build-cdn",
    "build": "shadow-cljs clj-run build.main/build",
    "upload": "node target/upload.js",
    "up": "yarn build-cdn && yarn upload",
    "page": "shadow-cljs clj-run build.main/page",
    "prepare": "yarn shadow-cljs release unimported",
    "ln": "cd target && rm -f entry && ln -s ../entry"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/calcit-workflow.git"
  },
  "bin": {
    "project-unimported": "./dist/unimported.js",
    "project-uppercase": "./dist/uppercase.js"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.12.0",
    "feather-icons": "^4.25.0",
    "shadow-cljs": "^2.8.83",
    "shortid": "^2.2.15"
  },
  "dependencies": {}
}
